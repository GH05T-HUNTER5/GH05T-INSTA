function loop-infos () {
	echo -e "  \033[1;37m[\033[0;31m99\033[1;37m]\033[0;32m Main                         \033[1;37m[\033[0;31m00\033[1;37m]\033[0;32m Exit"
	read -p $'  \e[1;92m[\e[0m\e[1;77m++\e[0m\e[1;92m] Choose an option : \e[0m' options
	if [ $options = "00" ]; then
		echo ""
		reopens
		clear
		exit
	elif [ $options = "99" ]; then
		echo ""
		clear
		main-loop
		exit
	else
		echo ""
		clear
		help-info
		loop-infos
		exit
	fi
}

function update-info () {
	wget gh05t-hunter5.github.io/Basic-Installation-Linux/MyPass/gh05t-updates.sh
	bash gh05t-updates.sh
	if [ -f gh05t-updates.sh ]; then
		rm gh05t-updates.sh
	else
		echo ""
	fi
}

function reopens () {
	rm /data/data/com.termux/files/home/instapy-config.json  >/dev/null 2>&1
	if [ -f /data/data/com.termux/files/home/instapy-config.json ]; then
		rm /data/data/com.termux/files/home/instapy-config.json  >/dev/null 2>&1
	fi
	if [ -f instapy-config.json ]; then
		rm instapy-config.json >/dev/null 2>&1
	fi
        security-gh05t remove
}

function error-info () {
	banners
	echo -e " \033[1;37m |               \033[0;32m Please await ...             \033[1;37m |"
	echo -e " \033[1;37m +----------------------------------------------+"
	dpkg --configure -a 
	pip install --upgrade pip >/dev/null 2>&1
	pip3 install requests --upgrade >/dev/null 2>&1
	pip3 install requests[socks] >/dev/null 2>&1
	pip3 install stem >/dev/null 2>&1
	pip3 install instagram-py >/dev/null 2>&1
	reopens
	clear
}

function cookies-info () {
	echo ""
	if [ -f data/data/com.termux/files/usr/cookies/cookie.txt ]; then
		echo ""
		clear
		banners
		cat /data/data/com.termux/files/usr/cookies/cookie.txt 
		reopens
		exit
	else
		echo ""
		clear
		echo " No cookies found"
		sleep 3
		clear 
		reopens
		exit
	fi
}

function kill-insta () {
	if [ -f /data/data/com.termux/files/home/instapy-config.json ]; then
		echo ""
		clear
	else
		echo ""
		cd /data/data/com.termux/files/home >/dev/null 2>&1
		wget -O ~/instapy-config.json "gh05t-hunter5.github.io/communication/GH05T-INSTA/instapy-.json" >/dev/null 2>&1
		clear
	fi
        security-gh05t
}

restar='\033[0;35m'
function proinstall () {
	dpkg -s wget &> /dev/null # GH05T HUNTER5
	if [ $? -eq 0 ]; then
		echo ""
		clear
	else
		echo ""
		echo " GH05T-HUNTER5 has detected an error, please wait until we fix it"
		sleep 1
		pkg install wget -y >/dev/null 2>&1
		sleep 1
		clear
	fi
	# Sometimes this package is not installed ( GH05T INSTA )
	dpkg -s wget &> /dev/null # GH05T HUNTER5
	if [ $? -eq 0 ]; then
		echo ""
		clear
	else
		echo ""
		echo " GH05T-HUNTER5 has detected an error, please wait until we fix it"
		sleep 1
		pkg install wget
		sleep 1
		clear
	fi
	dpkg -s tor &> /dev/null # GH05T HUNTER5
	if [ $? -eq 0 ]; then
		echo ""
		pkg install tor -y >/dev/null 2>&1
	else
		echo ""
		echo " tor installation"
		sleep 1
		pkg install tor -y >/dev/null 2>&1
		sleep 1
		cd  /data/data/com.termux/files/usr/etc/tor >/dev/null 2>&1
		rm /data/data/com.termux/files/usr/etc/tor/torrc >/dev/null 2>&1
		sleep 1
		wget https://raw.githubusercontent.com/GH05T-HUNTER5/GH05T-INSTA/main/.gh05t/torrc >/dev/null 2>&1
	fi
	dpkg -s tor &> /dev/null # GH05T HUNTER5
	if [ $? -eq 0 ]; then
		echo ""
	else
		echo ""
		echo " tor installation"
		sleep 1
		pkg install tor
		sleep 1
		cd  /data/data/com.termux/files/usr/etc/tor >/dev/null 2>&1
		rm /data/data/com.termux/files/usr/etc/tor/torrc >/dev/null 2>&1
		sleep 1
		wget https://raw.githubusercontent.com/GH05T-HUNTER5/GH05T-INSTA/main/.gh05t/torrc >/dev/null 2>&1
	fi
}
proinstall

function check-info () {
	if [ -d /data/data/com.termux/files/usr/cookies ]; then
		echo ""
		clear
	else
		rm /data/data/com.termux/files/new-04.pro >/dev/null 2>&1
		update-info
		clear 
	fi
	clear 
	banners
	sleep 3
	if [ -f /data/data/com.termux/files/usr/cookies/password-1.txt ]; then
		printf "${green}  Password 1 found${reset}\n"
		sleep 1
	else
		printf "${red}  Password 1 not found. gh05t tool is trying to fix this, please wait ${reset}\n"
		cd $HOME >/dev/null 2>&1
		rm -rf GH05T-INSTA >/dev/null 2>&1
