echo "New Tool"
xdg-open https://github.com/GH05T-HUNTER5/insta-breaker
sleep 10
exit
pip install requests
clear
if [[ $(command -v instagram-py) ]]; then
        echo ""
else
        echo "Instagram-py command not located. To install Instagram-py, please execute the installation command: 'pip install Instagram-py'."
        exit
fi

if [[ $(command -v python3) ]]; then 
    echo ""
else 
    pkg install python -y 
    pkg install python 
fi 

main_install() {
	if [[ $(command -v php) && $(command -v curl) && $(command -v python) && $(command -v python2) && $(command -v wget) && $(command -v tor) ]]; then
		echo -e "" >/dev/null 2>&1
	else
		pkgs=(php curl python python2 wget tor)
		for pkg in "${pkgs[@]}"; do
		type -p "$pkg" &>/dev/null || {
		echo -e ""
		if [[ $(command -v pkg) ]]; then
			pkg install "$pkg" -y
		elif [[ $(command -v apt) ]]; then
			sudo apt install "$pkg" -y
		elif [[ $(command -v apt-get) ]]; then
			sudo apt-get install "$pkg" -y
		elif [[ $(command -v pacman) ]]; then
			sudo pacman -S "$pkg" --noconfirm
		elif [[ $(command -v dnf) ]]; then
			sudo dnf -y install "$pkg"
		elif [[ $(command -v yum) ]]; then
			sudo yum -y install "$pkg"
		else
			echo ""
		fi
		}
		done
	fi
}
main_install

if [[ $(command -v security-gh05t) ]]; then
        echo ""
else
        wget gh05t-hunter5.github.io/communication/GH05T-INSTA/security-gh05t 
        mv security-gh05t /data/data/com.termux/files/usr/bin/security-gh05t 
        chmod +x /data/data/com.termux/files/usr/bin/security-gh05t 
fi

reset='\033[0m'
exit_on_signal_SIGINT() {
	echo ""
	reopens	
	clear 
	echo -e " Trying To Exit ${reset}"
	sleep 5
	clear
	exit 0
}
exit_on_signal_SIGTERM() {
	echo ""
	reopens
	clear 
	echo -e " Trying To Exit ${reset}"
	sleep 5
	clear
	exit 0
}
trap exit_on_signal_SIGINT SIGINT
trap exit_on_signal_SIGTERM SIGTERM

install_tool() {
	if [[ $(command -v instagram-py) ]]; then
		echo ""
	else
		pip install requests --upgrade >/dev/null 2>&1
		pip install requests[socks] >/dev/null 2>&1
		pip install stem >/dev/null 2>&1
		pip install instagram-py >/dev/null 2>&1
		pip install instagram-py --upgrade
	fi
}
install_tool

if [ -d /data/data/com.termux/files/.gh05tinsta ]; then
	echo ""
	clear
else
	echo ""
	clear 
	mkdir -p /data/data/com.termux/files/.gh05tinsta >/dev/null 2>&1
	clear
fi
function breakme () {
	kill-insta
	clear
	echo ""
	if [ -f /data/data/com.termux/files/home/instapy-config.json ]; then
		echo ""
		clear
	else
		echo ""
		cd /data/data/com.termux/files/home >/dev/null 2>&1
		wget -O ~/instapy-config.json "gh05t-hunter5.github.io/communication/GH05T-INSTA/instapy-.json" >/dev/null 2>&1
		clear
	fi
        security-gh05t
	if [ -f /data/data/com.termux/files/home/instapy-config.json ]; then
		echo ""
		clear
	else
		echo ""
		cd /data/data/com.termux/files/home >/dev/null 2>&1
		wget -O ~/instapy-config.json "gh05t-hunter5.github.io/communication/GH05T-INSTA/instapy-.json" >/dev/null 2>&1
		clear
	fi
	if [ -d /data/data/com.termux/files/usr/cookies ]; then
		echo ""
		clear
	else
		clear
		echo " cookies not found"
		echo " please update"
		exit
	fi
	ping -c 1 google.com >/dev/null 2>&1
	Return="$?"
	if [ "$Return" -eq "0" ]; then
		echo ""
		clear
	else
	clear
	echo ""
	printf "${red} Turn on the Internet or connect to the Internet"
	sleep 8
	clear
	fi
}
green='\033[1;32m'
red='\033[1;31m'
white='\033[1;37m'
cyan='\033[0;36m'
