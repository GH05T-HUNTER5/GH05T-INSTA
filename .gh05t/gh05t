	echo -e "  \033[1;37m[\033[0;31m02\033[1;37m]\033[0;32m Manual Attack"
	echo -e "  \033[1;37m[\033[0;31m03\033[1;37m]\033[0;32m Update"
	echo -e "  \033[1;37m[\033[0;31m04\033[1;37m]\033[0;32m Error"
