		cd /data/data/com.termux/files/usr/bin >/dev/null 2>&1
		chmod +x gh05t
	fi
	if [ -f /data/data/com.termux/files/usr/etc/tor/torrc ]; then
		echo ""
	else
		printf "${red} Tor Setup Please Wait"
