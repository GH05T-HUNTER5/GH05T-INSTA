	check-info
	exit
elif [ $1 = "--cookies" ] >/dev/null 2>&1
then
	echo ""
	clear
	cookies-info
	exit
elif [ $1 = "uninstall" ] >/dev/null 2>&1
then
	echo ""
	clear
	if [ $2 = "all" ]; then
		echo ""
		clear
		killmeall
	else
		clear
		echo " Try Again"
		sleep 4
		clear
		echo ""
		exit
	fi
	echo ""
else
	echo ""
	clear 
fi
